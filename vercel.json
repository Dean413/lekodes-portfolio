{
  "headers": [
    {
      "source": "/resume.pdf",
      "headers": [
        { "key": "Cache-Control", "value": "public,max-age=31536000,immutable" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/resume.pdf", "destination": "/resume.pdf" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
